{
  "name": "@saltcorn/plain-date",
  "version": "0.1.4",
  "description": "PlainDate: Date without time and timezone, drop-in compatible with Date",
  "homepage": "https://github.com/saltcorn/saltcorn/tree/master/packages/plain-date",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saltcorn/saltcorn.git",
    "directory": "packages/plain-date"
  },
  "scripts": {
    "test": "jest",
    "tsc": "tsc -p tsconfig.json",
    "clean": "echo 'No TypeScript build'",
    "test-cjs": "node test.js",
    "cjs-build": "esbuild ./index.js --format=cjs --outfile=index.cjs",
    "esm-build": "esbuild ./index.js --format=esm --outfile=esm/index.js"
  },
  "dependencies": {
    "esbuild": "^0.25.9"
  },
  "author": "Tom Nielsen",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/plugin_packages/",
      "/plugins_folder/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/plugin_packages/",
      "/plugins_folder/"
    ]
  },
  "type": "commonjs",
  "main": "./index.js",
  "module": "./esm/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./esm/index.js"
      },
      "require": {
        "types": "./index.d.ts",
        "default": "./index.js"
      }
    }
  },
  "types": "./index.d.ts",
  "bugs": {
    "url": "https://github.com/saltcorn/saltcorn/issues"
  }
}
