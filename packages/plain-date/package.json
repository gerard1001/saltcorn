{
  "name": "@saltcorn/plain-date",
  "version": "0.1.4",
  "description": "PlainDate: Date without time and timezone, drop-in compatible with Date",
  "homepage": "https://github.com/saltcorn/saltcorn/tree/master/packages/plain-date",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saltcorn/saltcorn.git",
    "directory": "packages/plain-date"
  },
  "scripts": {
    "test": "jest",
    "tsc": "tsc -p tsconfig.json",
    "clean": "echo 'No TypeScript build'",
    "build": "esbuild index.js --bundle --platform=node --format=cjs --outfile=dist/cjs/index.cjs && esbuild index.js --bundle --platform=node --format=esm --outfile=dist/esm/index.mjs",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build"
  },
  "dependencies": {},
  "author": "Tom Nielsen",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/plugin_packages/",
      "/plugins_folder/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/plugin_packages/",
      "/plugins_folder/"
    ]
  },
  "type": "commonjs",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "types": "index.d.ts",
  "typesVersions": {
    "*": {
      "esm/*": [
        "esm/*.d.ts"
      ],
      "*": [
        "*.d.ts"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
